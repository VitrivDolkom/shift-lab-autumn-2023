{
  "semi": false,
  "printWidth": 105,
  "endOfLine": "lf",
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "none",
  "importOrder": [
    "^@core/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@server/(.*)$",
    "^@/(api|components|uikit|assets|pages)(.*)/(.*)$",
    "^\\.(?!.*\\.(scss|css|png|jpg|svg)$).*",
    "(\\.\\.?/(\\w/?)+(\\.module)?.(scss|css)|\\..*(png|svg|jpg))"
  ],
  "importOrderCaseInsensitive": true
}
